import java.util.*;
import java.util.Scanner;


public class Numbers{
    public static void main(String[]args){
        int a;
        for (a=1;a<2;a++){
            System.out.println("The result one is: " + a);
        if (a == 1){
            System.out.println("The result two is: " + a);
        }
        }
    }
}
